{
  "name": "burgerbogo-api-server",
  "version": "1.0.0",
  "description": "버거보고 프로젝트 API 서버",
  "main": "index.js",
  "deploy" : {
    "development" : {
      "user" : "bitnami",
      "host" : ["api-dev.burgerbogo.net"],
      "ref"  : "origin/dev",
      "repo" : "git@github.com:NSM-inc/burgerbogo-api-server.git",
      "path" : "/burgerbogo-api-server",
      "ssh_options": "StrictHostKeyChecking=no",
      "post-deploy" : "npm install"
     },
     "production" : {
      "user" : "bitnami",
      "host" : ["api.burgerbogo.net"],
      "ref"  : "origin/prod",
      "repo" : "git@github.com:NSM-inc/burgerbogo-api-server.git",
      "path" : "/var/www/my-repository",
      "post-deploy" : "npm install"
     }
  },
  "scripts": {
    "start-watch": "pm2 deploy development setup start --interpreter babel-node app.js",
    "init": "babel-node init.js",
    "test": "env NODE_ENV=test mocha -r @babel/register -r @babel/polyfill --exit ",
    "start": "npm run init & npm run start-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NSM-inc/burgerbogo-api-server.git"
  },
  "author": "Jihye Kim, Minseok Seo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NSM-inc/burgerbogo-api-server/issues"
  },
  "homepage": "https://github.com/NSM-inc/burgerbogo-api-server#readme",
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.11.5",
    "aws-sdk": "^2.741.0",
    "babel-cli": "^7.0.0-0",
    "bcrypt": "^5.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "child-process": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.1.3",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.11",
    "path": "^0.12.7",
    "pm2": "^4.5.0",
    "sequelize": "^6.3.3",
    "sinon": "^9.2.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "xlsx": "^0.16.6"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules"
    ]
  }
}
